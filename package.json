  "scripts": {
    "build:server:prod": "webpack --config config/webpack.universal.server.prod.js  --progress --profile --bail",
    "build:client:prod": "webpack --config config/webpack.universal.browser.prod.js  --progress --profile --bail",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm-run-all build:server:prod build:client:prod",
    "clean:dist": "npm run rimraf -- dist",
    "serve": "npm run serve:universal",
    "serve:universal": "nodemon ./dist/server/server.js",
    "test": "karma start",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
